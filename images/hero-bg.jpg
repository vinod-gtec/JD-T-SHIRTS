<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="600" viewBox="0 0 1200 600">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#14213d;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="1200" height="600" fill="url(#bg-gradient)" />
  
  <!-- Decorative elements -->
  <g opacity="0.1" fill="#ffffff">
    <!-- Abstract clothing silhouettes -->
    <path d="M200,300 C250,250 300,350 350,300 C400,250 450,350 500,300" stroke="#ffffff" stroke-width="5" fill="none"/>
    <path d="M700,350 C750,300 800,400 850,350 C900,300 950,400 1000,350" stroke="#ffffff" stroke-width="5" fill="none"/>
    
    <!-- Decorative circles -->
    <circle cx="200" cy="200" r="50" />
    <circle cx="1000" cy="400" r="70" />
    <circle cx="600" cy="500" r="40" />
    <circle cx="400" cy="100" r="30" />
    <circle cx="800" cy="150" r="25" />
  </g>
  
  <!-- Subtle grid pattern -->
  <pattern id="grid" width="50" height="50" patternUnits="userSpaceOnUse">
    <path d="M50,0 L0,0 L0,50" fill="none" stroke="#ffffff" stroke-width="0.5" opacity="0.05"/>
  </pattern>
  <rect width="1200" height="600" fill="url(#grid)" />
</svg>